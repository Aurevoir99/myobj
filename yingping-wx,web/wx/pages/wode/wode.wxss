/* pages/wode/wode.wxss */
page{
  background-color: #eee;
}
.top{
  width: 100%;
  height: 400rpx;
  background-color: white;
  margin: 20rpx 0 0 0;
  display: flex;
  flex-direction: column;
}
.top image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  margin: auto;
}
.top navigator{
  width: 300rpx;
  height: 100rpx;
  background-color: rgb(34, 192, 34);
  border-radius: 50rpx;
  text-align: center;
  line-height: 100rpx;
  color: white;
  margin: auto;
}

.footer view{
  margin-top: 20rpx;
  background-color: white;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  line-height: 120rpx;
  padding: 0 20rpx;
}
